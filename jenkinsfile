  pipeline {
    agent any
    options {
        skipDefaultCheckout true
    } 
    stages {
        stage("SCM Checkout") {
            steps {
             script {
            checkout scm
             }
            }
        }

		stage ('Compile-Package'){
            steps {
		sh "mvn -version"
        sh "mvn clean install"
     }
    }
   }
}