  pipeline {
    agent any
    options {
        skipDefaultCheckout true
    } 
    stages {
        stage("SCM Checkout") {
            steps {
             script {
            checkout scm
             }
            }
        }

		stage ('Compile-Package'){
            steps {
		bat "mvn -version"
        bat  "mvn clean install"
     }
    }
   }
       post {
        always {
            archiveArtifacts artifacts: 'target/*.jar', fingerprint: true
        }
}