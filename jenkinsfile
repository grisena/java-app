  pipeline {
    agent any
    options {
        skipDefaultCheckout true
    } 
    stages {
        stage("SCM Checkout") {
            steps {
             script {
            checkout scm
             }
            }
        }

		stage ('Compile-Package'){
            steps {
		def mvnHome = tool name: 'maven-3', type: 'maven'
		sh "${mvnHome}/bin/mvn package"
     }
    }
   }
}